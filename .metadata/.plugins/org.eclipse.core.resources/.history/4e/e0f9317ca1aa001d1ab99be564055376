package com.spring.springexpressionlanguage;

import org.springframework.stereotype.Component;

@Component
public class Demo {
	private int x,y;

	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}
	
}
